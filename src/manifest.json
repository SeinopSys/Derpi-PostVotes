{
	"name": "Derpi-PostVotes",
	"version": "1.0.0",
	"description": "Vote on comments and forum posts on Derpibooru",
	"permissions": [
		"https://derpibooru.org/",
		"https://www.derpibooru.org/",
		"https://trixiebooru.org/"
	],
	"content_scripts": [
		{
			"matches": [
				"https://derpibooru.org/*",
				"https://www.derpibooru.org/*",
				"https://trixiebooru.org/*"
			],
			"js": [
				"js/socket.io.js",
				"js/derpi-postvotes.js"
			],
			"run_at": "document_end"
		}
	],
	"icons": {
		"16": "img/app-16.png",
		"32": "img/app-32.png",
		"48": "img/app-48.png",
		"128": "img/app-128.png"
	},
	"manifest_version": 2
}
